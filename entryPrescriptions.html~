  $.cookie('cookie-cli-imp', "");
  $.cookie('currentPatientInfo-bmi',"");
  $.cookie('currentPatientInfo-saturation',"");
  $.cookie('currentPatientInfo-pulse',"");
  $.cookie('drugPojoList',"");
